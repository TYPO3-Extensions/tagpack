		<type>module</type>
		<fileId>EXT:tagpack/locallang.xml</fileId>
	</meta>
	<data type="array">
		<languageKey index="default" type="array">
			<label index="and">and</label>
			<label index="taggedWith">with the tag</label>
			<label index="taggedWith2">with the tags</label>
			<label index="noItems">No matching</label>
			<label index="someItems">Matching</label>
			<label index="remove_item_from_list">Remove this tag from the list</label>
