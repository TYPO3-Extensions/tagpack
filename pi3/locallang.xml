                        <label index="ajaxgroupsearch_error_noTablesConfigured">Non è stata configurata alcuna tabella</label>
                        <label index="ajaxgroupsearch_error_noResults">Nessun risultato...</label>
                        <label index="ajaxgroupsearch_error_typeUnsupported">Tipo non supportato...</label>
                </languageKey>
	</data>
</T3locallang>
